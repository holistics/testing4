Dashboard khai_test {
  title: 'Khai Test'
  block v1: VizBlock {
    label: 'Total Quantity by Month Created At'
    viz: ColumnChart {
      dataset: khai_ecommerce
      calculation total_quantity {
        label: 'Total Quantity'
        formula: @aql sum(order_items.quantity);;
        calc_type: 'measure'
        data_type: 'number'
      }
      theme {

      }
      x_axis: VizFieldFull {
        ref: ref('orders', 'created_at')
        transformation: 'datetrunc month'
        format {
          type: 'date'
          pattern: 'LLL yyyy'
        }
      }
      y_axis {
        series {
          field: VizFieldFull {
            ref: 'total_quantity'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      }
      settings {
        row_limit: 5000
        x_axis_show_null_datetime: false
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block v2: VizBlock {
    label: 'Revenue by Month Created At'
    viz: DataTable {
      dataset: khai_ecommerce
      filter_groups: [
        OrFilters {
          filters: [
            VizFilter {
              field: ref('countries', 'name')
              operator: 'is'
              value: 'Vietnam'
            },
            VizFilter {
              field: ref('countries', 'name')
              operator: 'is'
              value: 'Singapore'
            }
          ]
        }
      ]
      fields: [
        VizFieldFull {
          ref: ref('orders', 'created_at')
          transformation: 'datetrunc month'
          format {
            type: 'date'
            pattern: 'LLL yyyy'
          }
        },
        VizFieldFull {
          ref: 'revenue'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 5000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  view: CanvasLayout {
    label: 'View 1'
    height: 1080
    grid_size: 20
    block v1 {
      position: pos(20, 20, 1160, 460)
      layer: 1
    }
    block v2 {
      position: pos(-20, 800, 1160, 460)
      layer: 1
    }
  }
  theme: H.themes.classic
}