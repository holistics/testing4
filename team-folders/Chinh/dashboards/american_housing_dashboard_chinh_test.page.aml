Dashboard american_housing_dashboard_chinh_test {
  title: 'American Housing Dashboard Chinh Test'
  description: ''

  view: CanvasLayout {
    label: 'View 1'
    width: 1300
    height: 1420
    grid_size: 20
    auto_expand_vertically: true
    mobile {
      mode: 'auto'
    }
    block v_t1pe {
      position: pos(20, 120, 1260, 680)
      layer: 1
    }
    block f_3ye5 {
      position: pos(20, 20, 220, 80)
      layer: 2
    }
    block v_s64q {
      position: pos(20, 820, 1260, 580)
      layer: 3
    }
    block t_0fo0 {
      position: pos(240, 20, 140, 80)
      layer: 4
    }
  }
  theme: H.themes.classic
  block v_t1pe: VizBlock {
    label: 'Map'
    viz: PointMap {
      dataset: american_housing_data_chinh_test
      lng: VizFieldFull {
        ref: r(zipcode_data_american_housing_data.longitude)
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      lat: VizFieldFull {
        ref: r(zipcode_data_american_housing_data.latitude)
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      value: VizFieldFull {
        ref: r(american_housing_data_chinh_test.total_price)
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      settings {
        point_size: 4
        value_colors {
          style: 'step'
          steps: 2
          min {
            color: '#18A59E'
          }
          max {
            color: '#D70909'
          }
        }
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block f_3ye5: FilterBlock {
    label: 'Slider '
    type: 'field'
    source: FieldFilterSource {
      dataset: american_housing_data_chinh_test
      field: r(param_model.param_slider)
    }
    default {
      operator: 'is'
      value: '1145000'
    }
  }
  block v_s64q: VizBlock {
    label: 'Table'
    viz: DataTable {
      dataset: american_housing_data_chinh_test
      fields: [
        VizFieldFull {
          ref: r(zipcode_data_american_housing_data.latitude)
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: r(zipcode_data_american_housing_data.longitude)
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: r(american_housing_data_chinh_test.total_price)
          format {
            type: 'number'
            pattern: 'inherited'
          }
          uname: 'custom_total_price_1'
        },
        VizFieldFull {
          ref: r(american_housing_data_chinh_test.divider)
          format {
            type: 'number'
            pattern: 'inherited'
          }
          uname: 'custom_divider'
        }
      ]
      settings {
        pagination_size: 500
        show_row_number: true
        sorts: [
          SortSetting {
            key: 'custom_total_price_1'
            direction: 'asc'
          }
        ]
        conditional_formats: [
          ConditionalFormat {
            key: 'custom_divider'
            format: SingleFormat {
              condition {
                operator: 'is'
                value: 'LOW'
              }
              text_color: '#328159'
              background_color: '#B5E3CD'
            }
          },
          ConditionalFormat {
            key: 'custom_divider'
            format: SingleFormat {
              condition {
                operator: 'is'
                value: 'HIGH'
              }
              text_color: '#BF0E08'
              background_color: '#FAD2D1'
            }
          }
        ]
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block t_0fo0: TextBlock {
    content: @md <div style="background-color: #D70909; padding: 3px; color: white; text-align: center;">HIGH</div>
<div style="background-color: #18A59E; padding: 3px; color: white;text-align: center;">LOW</div>;;
  }
}