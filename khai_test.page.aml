Dashboard khai_test {
  title: 'Khai Test'
  block v1: VizBlock {
    label: 'Revenue by Month Created At'
    viz: DataTable {
      dataset: khai_ecommerce
      conditions: [
        "countries.name == 'Vietnam'"
      ]
      fields: [
        VizFieldFull {
          ref: ref('orders', 'created_at')
          transformation: 'datetrunc month'
          format {
            type: 'date'
            pattern: 'LLL yyyy'
          }
        },
        VizFieldFull {
          ref: 'revenue'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 5000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  interactions: []
  view: CanvasLayout {
    label: 'View 1'
    height: 500
    grid_size: 20
    block v1 {
      position: pos(20, 20, 1160, 460)
      layer: 1
    }
  }
  theme: H.themes.classic
}