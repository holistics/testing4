  settings {
    timezone: 'America/Los_Angeles'
    cache_duration: 10
  }
  interactions: [
    FilterInteraction {
      from: 'v1'
      to: [
        CustomMapping {
          block: 'v15'
          disabled: true
        }
      ]
    },
    FilterInteraction {
