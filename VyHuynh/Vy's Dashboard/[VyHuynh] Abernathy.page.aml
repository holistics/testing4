          alignment: 'right'
          group_values_into: 10
        }
        series {
          mark_type: 'line'
          field: VizFieldFull {
            ref: r(vyht_ecommerce_orders_1.id)
            aggregation: 'count distinct'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
          settings {
            color: '#18A59E'
          }
        }
